{
	"compilerOptions": {
		"module": "none",
		"target": "es2022",
		"noImplicitAny": true,
		"removeComments": false,
		"preserveConstEnums": false,
		// "sourceMap": true,
		"outDir": "out",
		"jsx": "react",
		"jsxFactory": "_.create",
		"rootDir": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"strict": true,
		"strictNullChecks": true
	},
	"include": [
		"Nitro.d.ts",
		"ts/**/*",
	],
	"exclude": [],
	"lib": ["dom", "es2017"]
}
